[DISABLE]

{$lua}
--//Restore values to default

--[[Restore controller input]]
WriteBytes(0x203F70B8, 48, 125, 105, 00)

--[[Restore controller input. TODO]]
--writeBytes(ground_reflection, 0)

--[[Restore disabled HUD elements]]
