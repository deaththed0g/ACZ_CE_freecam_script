		zpos.Type = vtSingle
		zpos.appendToEntry(m)

		ypos.Description = "Y coordinates"
		ypos.setAddress(yposoff)
		ypos.Type = vtSingle
		ypos.appendToEntry(m)

		yrotation.Description = "Pitch"
